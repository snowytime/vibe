{
    "name": "@snowytime/vibe",
    "type": "module",
    "sideEffects": false,
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "rm -rf dist && swc src --config-file build.json -d dist && tsc --p tsconfig.api.json && tsc --p tsconfig.client.json"
    },
    "author": "snowytime",
    "license": "MIT",
    "engines": {
        "node": ">=16.0.0"
    },
    "bin": {
        "vibe": "./dist/cli/index.js"
    },
    "files": [
        "dist",
        "app"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./client": "./app/src/exports.ts"
    },
    "typesVersions": {
        "*": {
            "client": [
                "./dist/src/exports.d.ts"
            ]
        }
    },
    "dependencies": {
        "@snowytime/css": "^0.0.31",
        "@snowytime/react-magic": "^0.0.14",
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.44",
        "@vitejs/plugin-react": "^3.1.0",
        "chokidar": "^3.5.3",
        "commander": "^10.0.0",
        "express": "^4.18.2",
        "get-port": "^6.1.2",
        "globby": "^13.1.3",
        "picocolors": "^1.0.0",
        "react-frame-component": "^5.2.6",
        "react-router-dom": "^6.10.0",
        "sass": "^1.60.0",
        "vite": "^4.2.1",
        "vite-tsconfig-paths": "^4.0.8"
    },
    "peerDependencies": {
        "react": ">=16.14.0",
        "react-dom": ">=16.14.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.17",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.33",
        "@types/react-dom": "^18.0.11",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.0.3"
    },
    "imports": {
        "#debug/*": "./src/debug/*",
        "#log/*": "./src/log/*",
        "#types/*": "./src/types/*",
        "#finders/*": "./src/finders/*",
        "#config/*": "./src/config/*",
        "#helpers/*": "./src/helpers/*",
        "#structures/*": "./src/structures/*",
        "#parsers/*": "./src/parsers/*",
        "#server/*": "./src/server/*",
        "#plugin/*": "./src/plugin/*"
    }
}
